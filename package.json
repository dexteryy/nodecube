{
  "name": "nodecube",
  "description": "nodecube is a batch of continuously updated base code and configurations for the minimal modern node.js service that conform to RESTful API, 12-Factor App, Microservice Architecture",
  "author": "dexteryy <dexter.yy@gmail.com>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dexteryy/nodecube/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/dexteryy/nodecube",
  "repository": "https://github.com/dexteryy/nodecube",
  "version": "0.5.1",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run lint && npm run build",
    "precommit": "npm run lint && npm run test",
    "build": "if [ -d build ]; then rm -r build; fi; npm run build:src",
    "build:src": "babel src --out-dir build --source-maps",
    "test": "npm run build && npm run test:nobuild",
    "test:nobuild": "mocha build/**/*.test.js",
    "lint": "eslint src/**"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=6"
  },
